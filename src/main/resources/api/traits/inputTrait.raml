#%RAML 1.0 Trait
queryParameters:
      customerName:
        required: true
        minLength: 4
        maxLength: 40
      bank:
        required: true
        enum:
          - HDFC
          - ICICI
          - SBI
          - AXIS
      accountType:
        required: true
        enum:
          - savings
          - current
      branchName:
        required: true
        minLength: 4
        maxLength: 10